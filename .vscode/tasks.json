{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Watch XDE.Team",
            "command": "dotnet",
            "type": "process",
            "options": {
                "cwd": "${workspaceFolder}/src"
            },
            "args": [
                "watch",
                "--project",
                "Xde.Team"
            ],
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "Publish XDE.Team",
            "command": "dotnet",
            "type": "process",
            "options": {
                "cwd": "${workspaceFolder}/src"
            },
            "args": [
                "publish",
                "Xde.Team",
                "--output",
                "e:/WWW/xde.team"
            ],
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build"
            }
        }
    ]
}